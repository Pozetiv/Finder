<div class="nested-fields">
  <div class="field">
    <div class="control">
      <%= f.input :started, as: :date, input_html: { class: 'select' }, wrapper: false,
          order: [:month, :year] %>
    </div>
  </div>



  <div class="field">
    <div class="control">
      <%= f.input :finished, as: :date, input_html: { class: 'select' }, wrapper: false,
          order: [:month, :year] %>
    </div>
  </div>
  <div class="field">
    <div class="control">
      <%= f.input :position, label: 'You position', input_html: { class: 'input' },
           label_html: { class: 'label'}, wrapper: false %>
    </div>
  </div>
  <div class="field">
    <div class="control">
      <%= f.input :company_name, input_html: { class: 'input' }, wrapper: false, label_html: { class: 'label'} %>
    </div>
  </div>
  <div class="field">
    <div class="control">
      <%= f.input :address, input_html: { class: 'input' }, wrapper: false, label_html: { class: 'label'},
          hint: 'Example: Washington, D.C.' %>
    </div>
  </div>
  <div class="field">
    <div class="control">
      <%= f.input :body, as: :text, input_html: { class: 'textarea' }, wrapper: false,
       label: 'Additional information', placeholder: 'Some word about what you do' %>
    </div>
  </div>

    <%= link_to_remove_association "remove Education", f, class: 'button' %>
</div>
